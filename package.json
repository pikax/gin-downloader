{
  "name": "gin-downloader",
  "version": "1.0.0",
  "description": "Simple manga scrapper for famous online manga websites.",
  "keywords": [
    "manga",
    "downloader",
    "crawler",
    "manga-downloader",
    "gin-manga",
    "manga-fox",
    "mangafox",
    "mangapanda",
    "mangahere"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "babel -d lib/ src/",
    "nyc": "nyc node_modules/mocha/bin/_mocha test/**/*.spec.js",
    "lint": "eslint ./src/*/**.js ./test/*/**.spec.js  --quiet; exit 0",
    "test:watch": "mocha test/**/*.spec.js --colors --watch",
    "mocha": "mocha test/**/*.spec.js --colors",
    "test:debug": "cross-env DEBUG=\"gin-downloader:*,-gin-downloader:*:verbose\" istanbul cover node_modules/mocha/bin/_mocha  test/**/*.spec.js -- -R spec",
    "test": "istanbul cover node_modules/mocha/bin/_mocha  test/**/*.spec.js -- -R spec"
  },
  "repository": "https://github.com/pikax/gin-downloader",
  "author": "pikax",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pikax/gin-downloader/issues"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "homepage": "https://github.com/pikax/gin-downloader#readme",
  "devDependencies": {
    "@types/bluebird": "~3.5.0",
    "@types/chai": "^3.4.35",
    "@types/chai-as-promised": "^0.0.30",
    "@types/debug": "~0.0.29",
    "@types/es6-promise": "^0.0.32",
    "@types/libxmljs": "^0.14.30",
    "@types/lodash": "^4.14.57",
    "@types/node": "^7.0.10",
    "babel-core": "~6.24.0",
    "babel-plugin-istanbul": "~4.0.0",
    "babel-preset-es2015": "~6.24.0",
    "chai": "~3.5.0",
    "chai-as-promised": "~6.0.0",
    "coveralls": "~2.12.0",
    "cross-env": "~3.2.4",
    "eslint": "~3.18.0",
    "istanbul": "v1",
    "istanbul-coveralls": "~1.0.3",
    "mocha": "~3.2.0",
    "mocha-lcov-reporter": "~1.3.0",
    "nyc": "~10.1.2",
    "typescript": "^2.2.1"
  },
  "dependencies": {
    "bluebird": "~3.5.0",
    "debug": "~2.6.3",
    "libxmljs": "~0.18.4",
    "lodash": "~4.17.4",
    "no-case": "~2.3.1",
    "requestretry": "~1.12.0"
  }
}
